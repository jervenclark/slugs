    "license": "MIT",
    "authors": [
        {
            "name": "Jerven Clark Chua",
            "email": "jervenclark@gmail.com",
            "homepage": "http://github.com/jervenclark"
        }
    ],
    "homepage": "https://github.com/jervenclark/slugs",
    "keywords": ["Laravel", "Slugs"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Jervenclark\\Slugs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jervenclark\\Slugs\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jervenclark\\Slugs\\SlugsServiceProvider"
            ],
            "aliases": {
                "Slugs": "Jervenclark\\Slugs\\Facades\\Slugs"
            }
        }
    }
}
